<panel size='400' color='0' interval='1' context='menu'>
    <label>@Server list</label>
    <panel id="server_list" size="400" padding="5" color="#FFFFFF11" max-length="400">
    </panel>

    <button onclick='refresh_server_list()'>@Refresh Server List</button>
    <button onclick='menu.page="add_server"'>@Add Server</button>
    <button onclick='menu.page="profile"'>@Profile</button>
    <button onclick='events.emit("multiplayer:disconnect")'>@Back to Main Menu</button>
    
</panel>
